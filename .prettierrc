{
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 100,
  "importOrder": [
    "assert",
    "^@cognixus/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@vitejs/(.*)$",
    "vitest-mock-extended",
    "#(.*)$",
    "#app",
    "^[./]",
    "^@/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "semi": false,
  "singleQuote": true
}
